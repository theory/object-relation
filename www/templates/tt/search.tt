[% 
   WRAPPER shared/main.tt
   title = 'Search' 
%]
  [% USE Renderer( mode => 'edit', format => {} ) %]
  <div class="search">
    <fieldset>
      <legend>[% search_key %]</legend>
      <table>
        [% FOREACH attr = class.attributes %]
        <tr>
          <td><label for="[% attr.name %]">[% attr.name %]</label></td>
          <td>[% Renderer.render(attr) %]</td>
        </tr>
        [% END %]
      </table>
    </fieldset>
  <div>
[% END %]
